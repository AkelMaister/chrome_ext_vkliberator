{
	"manifest_version": 2,
	"version": "1.0",

	"background": {
		"scripts": [
			"js/proxy.js"
		]
	},
	"browser_action": {
		"default_title": "__MSG_title__"
	},
	"icons": {
		"16": "img/icon16.png",
		"32": "img/icon32.png",
		"48": "img/icon48.png",
		"128": "img/icon128.png"
	},
   
	"default_locale": "ru",

	"description": "__MSG_description__",
	"name": "__MSG_name__",

	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*/*",
		"proxy",
		"storage",
		"tabs"
	]
}
